generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client" // oder ein anderer sauberer Pfad
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Book {
  id              Int    @id @default(autoincrement())
  title           String
  author          String
  publicationYear Int
}
